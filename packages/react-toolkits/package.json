{
  "name": "react-toolkits",
  "version": "0.8.16",
  "packageManager": "^pnpm@8.7.5",
  "sideEffects": [
    "**/*.css"
  ],
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    "./style.css": "./lib/style.css",
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./locales/*": {
      "import": "./locales/*.js",
      "types": "./locales/*.d.ts"
    }
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "lib/",
    "locales/"
  ],
  "license": "MIT",
  "scripts": {
    "build": "pnpm run build:prod",
    "build:prod": "cross-env NODE_ENV=\"production\" ts-node --project tsconfig.node.json scripts/build.ts",
    "build:dev": "cross-env NODE_ENV=\"development\" ts-node --project tsconfig.node.json scripts/build.ts",
    "lint": "eslint --fix --ext .ts,.tsx,.js,.jsx ."
  },
  "author": "chaos1ee <chaos.maybeeee@gmail.com>",
  "homepage": "https://github.com/chaos1ee/everythingflow/tree/main/packages/react-toolkits#readme",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/chaos1ee/everythingflow.git",
    "directory": "packages/react-toolkits"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "dayjs": "^1.11.10",
    "immer": "^10.0.3",
    "jwt-decode": "^4.0.0",
    "lodash-es": "^4.17.21",
    "query-string": "^8.1.0",
    "swr": "^2.2.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@flow97/eslint-config-mono": "workspace:*",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "antd": "^5.12.4",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "glob": "^10.3.10",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.0",
    "rollup-plugin-dts": "^6.1.0",
    "tailwind-config": "workspace:*",
    "tailwindcss": "^3.4.0",
    "ts-essentials": "^9.4.1",
    "ts-node": "^10.9.2",
    "tsconfig": "workspace:*",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.6.4"
  },
  "peerDependencies": {
    "antd": "^5.12.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2"
  }
}
