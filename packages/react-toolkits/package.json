{
  "name": "react-toolkits",
  "version": "0.4.2",
  "packageManager": "^pnpm@8.7.5",
  "sideEffects": [
    "**/*.css"
  ],
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.mts",
  "exports": {
    "./style.css": "./lib/index.css",
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.mts"
    },
    "./locales/*": {
      "import": "./locales/*.js",
      "types": "./locales/*.d.mts"
    }
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "lib/",
    "locales/"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "lint": "eslint --fix --ext .ts,.tsx,.js,.jsx ."
  },
  "author": "chaos1ee <chaos.maybeeee@gmail.com>",
  "homepage": "https://github.com/chaos1ee/everythingflow/tree/main/packages/react-toolkits#readme",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/chaos1ee/everythingflow.git",
    "directory": "packages/react-toolkits"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "antd": "^5.11.3",
    "dayjs": "^1.11.10",
    "events": "^3.3.0",
    "immer": "^10.0.3",
    "jwt-decode": "^4.0.0",
    "lodash-es": "^4.17.21",
    "query-string": "^8.1.0",
    "swr": "^2.2.4",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@flow97/eslint-config-mono": "workspace:*",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.9.5",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "tailwind-config": "workspace:*",
    "tailwindcss": "^3.3.5",
    "ts-essentials": "^9.4.1",
    "tsconfig": "workspace:*",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
