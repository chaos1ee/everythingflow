{
  "name": "everythingflow",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "turbo dev --filter=\"react-web\" --no-daemon",
    "build": "turbo build",
    "clean": "turbo clean",
    "lint": "turbo lint",
    "preinstall": "npx only-allow pnpm",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clear": "rimraf -g 'apps/*/node_modules' && rimraf -g 'packages/*/node_modules' && rimraf node_modules"
  },
  "repository": "git@github.com:chaos1ee/everythingflow.git",
  "author": "chaos1ee <chaos.maybeeee@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.2",
    "@flow97/prettier-config": "workspace:*",
    "@types/node": "^20.4.5",
    "eslint": "^8.45.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "turbo": "^1.10.12"
  },
  "prettier": "@flow97/prettier-config"
}
