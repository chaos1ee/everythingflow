{
  "name": "react-toolkits",
  "version": "0.3.34",
  "packageManager": "^pnpm@8.7.5",
  "sideEffects": [
    "**/*.css"
  ],
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.mts",
  "exports": {
    "./style.css": "./lib/index.css",
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.mts"
    },
    "./locales/*": {
      "import": "./locales/*.js",
      "types": "./locales/*.d.mts"
    }
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "lib/",
    "locales/"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "lint": "eslint --fix --ext .ts,.tsx,.js,.jsx ."
  },
  "author": "chaos1ee <chaos.maybeeee@gmail.com>",
  "homepage": "https://github.com/chaos1ee/everythingflow/tree/main/packages/react-toolkits#readme",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/chaos1ee/everythingflow.git",
    "directory": "packages/react-toolkits"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@ant-design/icons": "^5.1.4",
    "antd": "^5.7.3",
    "dayjs": "^1.11.9",
    "events": "^3.3.0",
    "immer": "^10.0.2",
    "jwt-decode": "^3.1.2",
    "lodash-es": "^4.17.21",
    "query-string": "^8.1.0",
    "swr": "^2.2.0",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@flow97/eslint-config-mono": "workspace:*",
    "@types/lodash-es": "^4.17.8",
